P.register('Device', function() {

    var device = {
        
        isKindle     : Boolean(false),
        isAndroid    : Boolean(),
        isIPhone     : Boolean(false),
        isIPad       : Boolean(false),
        isMobile     : Boolean(false),
        isTablet     : Boolean(),

        isWebkit     : Boolean(),
        isSafari     : Boolean(),
        isIE         : Boolean(false),
        isIEModern   : Boolean(),
        isIE8        : Boolean(),
        isIE7        : Boolean(),
        isFirefox    : Boolean(false),

        isWindows    : Boolean(false),
        isMac        : Boolean(false)
    };
    device.isDesktop = !(device.isMobile || device.isTablet);
    device.isIOS = device.isIPhone || device.isIPad;
    device.isPopupSupported = device.isDesktop;
    device.isDesktopAppSupported = device.isWindows || device.isMac;
    device.showNativeScrollBars = (!device.isDesktop);

    device.embeddedDeviceType = "";
    device.isEmbeddediOS = device.isIOS;


    device.canViewPdf = Boolean(
        (function() {
            //Tests specifically for Adobe Reader (aka Acrobat) in Internet Explorer
            var hasReaderActiveX = function () {
                // standard program ids for Adobe readers
                var progIDs = [
                    "AcroPDF.PDF",
                    "AcroPDF.PDF.1",
                    "PDF.PdfCtrl", 
                    "PDF.PdfCtrl.5",
                    "PDF.PdfCtrl.1"
                    ];

                // 3rd party PDF Reader ActiveX Controls for IE, like nitro and foxit
                var prodIDs3rd = [
                    "NitroPDF.IE.ActiveDoc",                    //Nitro
                    "PDFXCviewIEPlugin.CoPDFXCviewIEPlugin",    //PDF XChange Viewer
                    "PDFXCviewIEPlugin.CoPDFXCviewIEPlugin.1",  //PDF XChange Viewer
                    "FoxitReader.FoxitReaderCtl",               //foxit
                    "FoxitReader.FoxitReaderCtl.1",              //foxit
                    "FOXITREADEROCX.FoxitReaderOCXCtrl",        //foxit
                    "FOXITREADEROCX.FoxitReaderOCXCtrl.1"       //foxit
                    ];

                var tryCreatingActiveXObj = function(progIDs) {
                    var activeXObject = null;
                    for (var i=0; i<progIDs.length; i++) {
                        try {
                            activeXObject = new window.ActiveXObject(progIDs[i]);
                        } catch(e) {
                            activeXObject = null;
                        }
                        if (activeXObject) {
                            break;
                        }
                    }
                    return activeXObject;
                }
                return tryCreatingActiveXObj(progIDs) || tryCreatingActiveXObj(prodIDs3rd);
            };
            window.hasReaderActiveX = hasReaderActiveX;
            //Tests specifically for Adobe Reader (aka Adobe Acrobat) in non-IE browsers
            var hasReader = function () {
                var i,
                    regx = /Adobe Reader|Adobe PDF|Acrobat/gi;
                if (navigator.plugins) {
                    for(i=0; i<navigator.plugins.length; i++) {
                        if(regx.test(navigator.plugins[i].name)){
                            return true;
                        }
                    }
                }
                return false;
            };

            //Detects any PDF support based on mimetype, can cover third party pdf viewers like foxit
            //application/pdf - covers standard
            //application/vnd.adobe.pdfxml
            var hasGeneric = function () {
                var mimeTypes = ["application/pdf", "application/vnd.adobe.pdfxml", "application/vnd.adobe.x-mars", "application/vnd.adobe.xfdf"];
                var hasGenericPDFSupport = false;
                for (var i=0; i<mimeTypes.length; i++) {
                    try {
                        var plugin = navigator.mimeTypes[mimeTypes[i]];
                        if (plugin && plugin.enabledPlugin) {
                            hasGenericPDFSupport = true;
                            break;
                        }
                    } catch (e){}
                }
                return hasGenericPDFSupport;
            };

            return hasReader() || hasReaderActiveX() || hasGeneric();
        }())
    );

    return device;
});


P.register('_FeatureFlags', function() {
    function FeatureFlags(flags) {
        this.flags = flags || {};
    }

    FeatureFlags.prototype = {
        set:     function(flag, value) { this.flags[flag] = Boolean(value); },
        get:     function(flag) { return this.flags[flag]; },
        disable: function(flag) { this.flags[flag] = false; },
        enable:  function(flag) { this.flags[flag] = true; },
        isOn:    function(flag) { return this.get(flag) !== false; },
        isOff:   function(flag) { return this.get(flag) === false; },
        putAll:  function(flagMap) {
            for (var key in flagMap) {
                this.set(key, flagMap[key]);
            }
        }
    };

    return new FeatureFlags();
});


